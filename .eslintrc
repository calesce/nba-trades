{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "indent": [2, 2],
    "brace-style": [2, "stroustrup", { "allowSingleLine": false }],
    "camelcase": [2, { "properties": "always" }],
    "strict": 0
  }
}
